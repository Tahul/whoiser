{
  "name": "@tahul/whoiser",
  "type": "module",
  "version": "2.0.3",
  "packageManager": "pnpm@8.6.12",
  "description": "Whois info for TLDs, domains and IPs",
  "author": "Andrei Igna <andrei@laye.red>",
  "license": "MIT",
  "homepage": "https://github.com/LayeredStudio/whoiser#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LayeredStudio/whoiser.git"
  },
  "bugs": {
    "url": "https://github.com/LayeredStudio/whoiser/issues"
  },
  "keywords": [
    "whois",
    "domain",
    "tld",
    "ip"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild -c build.config.ts",
    "stub": "unbuild -c build.config.ts --stub",
    "lint": "eslint --cache --quiet",
    "lint:fix": "eslint --cache --fix --quiet",
    "release": "release-it",
    "test": "pnpm lint && vitest run --coverage",
    "test:ui": "vitest --ui --open=false",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "punycode": "2.3.1",
    "socks": "^2.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.9.0",
    "@types/node": "^20.11.30",
    "@types/punycode": "^2.1.4",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.57.0",
    "release-it": "^17.1.1",
    "typescript": "^5.4.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.4.0"
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm build",
        "pnpm test"
      ]
    },
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
